{
  "name": "pixidos/gpwebpay",
  "license": ["BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
  "type": "library",
  "description": "GP WepPay integration for Nette Framework",
  "keywords": ["nette", "gp", "gp webpay","webpay", "pay", "checkout", "payment"],
  "homepage": "https://github.com/pixidos",

  "authors": [
    {
      "name": "Ondra Votava",
      "email": "ondra@votava.dev"
    }
  ],
  "support": {
    "issues": "https://github.com/Pixidos/GPWebPay/issues"
  },
  "require": {
    "php": "~7.1",
    "ext-openssl": "*",
    "nette/di": "^2.4.10 || ^3.0",
    "nette/application": "^2.4.10 || ^3.0",
    "nette/utils": "^2.5 || ^3.0",
    "latte/latte": "^2.4",
    "pixidos/gpwebpay-core": "~2.0"
  },
  "require-dev": {
    "nette/http": "^2.4.10 || ^3.0",
    "nette/caching": "^2.5 || ^3.0",
    "nette/finder": "^2.5",
    "nette/tester": "^2.3",
    "pixidos/coding-standards": "~0.1 || ~1.0",
    "phpstan/phpstan-nette": "~0.11"
  },
  "autoload": {
    "psr-4": {
      "Pixidos\\GPWebPay\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GPWebPay\\Tests\\": "tests/"
    }
  }
}
