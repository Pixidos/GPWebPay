{
  "name": "pixidos/gpwebpay",
  "license": ["BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
  "type": "library",
  "description": "GP WepPay integration for Nette Framework",
  "keywords": ["nette", "gp", "gp webpay","webpay", "pay", "checkout", "payment"],
  "homepage": "https://github.com/pixidos",

  "authors": [
    {
      "name": "Ondra Votava",
      "email": "ondra@votava.dev"
    }
  ],
  "support": {
    "issues": "https://github.com/Pixidos/GPWebPay/issues"
  },
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^7.2 || ^8.0.0",
    "ext-openssl": "*",
    "ext-json": "*",
    "nette/di": "^2.4.10 || ^3.0",
    "nette/application": "^2.4.16 || ^v3.0.6",
    "nette/utils": "^v2.5.3 || ^3.2",
    "latte/latte": "^2.4",
    "pixidos/gpwebpay-core": "~2.1",
    "nette/bootstrap": "^3.1"
  },
  "require-dev": {
    "nette/http": "^2.4.10 || ^3.0",
    "nette/caching": "^2.5 || ^3.0",
    "nette/finder": "^2.5",
    "nette/tester": "^2.4",
    "pixidos/coding-standards": ">=1.1.3",
    "phpstan/phpstan-nette": "^0.12",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Pixidos\\GPWebPay\\": "src/"
    },
    "files": [
      "src/aliases.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "GPWebPay\\Tests\\": "tests/"
    }
  }
}
